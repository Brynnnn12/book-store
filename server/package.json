{
  "name": "bookstore-pdf-backend",
  "version": "1.0.0",
  "description": "Backend API untuk aplikasi Bookstore PDF",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm run dev --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "nodemon seeder/adminSeeder.js",
    "data:destroy": "node seeder.js -d"
  },
  "keywords": [
    "bookstore"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.21.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.1",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1"
  }
}
